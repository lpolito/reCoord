{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "nodemon --exec babel-node ./src/index.mjs",
    "start": "yarn run prod",
    "server": "babel-node ./src/index.mjs",
    "server:prod": "node ./build/index.mjs",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf build",
    "build": "babel ./src --out-dir build --keep-file-extension",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.10"
  }
}
