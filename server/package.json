{
  "name": "server",
  "version": "0.0.0",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf build",
    "build": "yarn clean && tsc",
    "serve": "node build/",
    "watch-node": "nodemon build/",
    "watch-ts": "yarn clean && tsc -w",
    "watch": "yarn clean && tsc-watch --onSuccess \"yarn serve\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.17.0",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.6.9",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "nodemon": "^1.18.10",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.5.3"
  }
}
